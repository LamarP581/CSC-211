/**
 *
 * @author itlabs
 */
public class Fibrecursive {

    public static int fibonacci(int n){
        if (n <= 1){
            return n;
        }
        return fibonacci(n - 1) + fibonacci (n - 2);
    }
    public static void main(String[] args) {
      int n = 10;
     System.out.println("Recursive Fibonacci of " + n + " is " + fibonacci(n));
    }
    
}

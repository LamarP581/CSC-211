

public abstract class Animal {
    int age;
    String name;
    String breed;
    
    Animal(String name,int age,String breed){
        this.age = age;
        this.name = name;
        this.breed = breed;
    }
    public void printInfo(){
        System.out.println("My name is " + name + " and I'm " + age + ".I AM A " + breed);
    }
public abstract void makenosie();
    
}
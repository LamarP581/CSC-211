/**
 *
 * @author itlabs
 */
public class Shape {
private int numofPoints;
private int numofSides;



public Shape (int numofPoints, int numofSides){
    this.numofPoints = numofPoints;
    this.numofSides = numofSides;
}



public int getNumofpoints(){
    return numofPoints;
}


public int getNumofsides(){
    return numofSides;
}
public void setnumsidesOrpoints(int numofPoints, int numofSides){
    this.numofPoints = numofPoints;
    this.numofSides = numofSides;
}
}
